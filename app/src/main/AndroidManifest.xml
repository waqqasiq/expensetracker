<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.expensetrackauth">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_tk_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_tk_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UpdateExpActivity"></activity>
        <activity android:name=".SigninEmail" />
        <activity android:name=".SummaryActivity" />
        <activity android:name=".AddExpenses" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".SigninActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>